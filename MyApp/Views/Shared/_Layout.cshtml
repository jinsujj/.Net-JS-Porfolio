<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <meta name="description" content="Full Stack Runner Blog">
    <meta property="og:title" content="Summary" />
    <meta property="og:description" content="Full Stack Runner Blog" />
    <meta property="og:image" content="https://jinsu-blog.iptime.org/files/PortfolioMeta.jpg" />

    <title>@ViewBag.Title</title>

    <!-- include JQuery -->
    <script src="https://code.jquery.com/jquery-3.5.1.min.js" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js"></script>

    <!-- Material -->
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link rel="stylesheet" href="~/assets/css/materialize.css">
    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.5/js/materialize.min.js"></script>

    <!-- include BootStrap -->
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css">
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js" crossorigin="anonymous"></script>

    <!-- include SummerNote -->
    <link href="https://cdn.jsdelivr.net/npm/summernote@0.8.18/dist/summernote-bs4.min.css" rel="stylesheet">
    <script src="https://cdn.jsdelivr.net/npm/summernote@0.8.18/dist/summernote-bs4.min.js"></script>
    <link href="/js/summernote-bs4.min.css" rel="stylesheet">

    <!-- include SummerNote hightlight plugin -->
    <script src="~/js/summernote-ext-syntax.js"></script>
    <script src="~/js/summernote-ko-KR.js"></script>

    <!-- Prism-->
    <link href="/assets/css/prism_coy.css" rel="stylesheet" />

    <!-- Toast Grid-->
    <link rel="stylesheet" href="https://uicdn.toast.com/grid/latest/tui-grid.css" />

    <!-- Google AdSense-->
    <script data-ad-client="ca-pub-2309486098831986" async src="https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>


    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=G-J45TMJEMFN"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag() { dataLayer.push(arguments); }
        gtag('js', new Date());

        gtag('config', 'G-J45TMJEMFN');
    </script>

    <link href="~/assets/css/index.css" rel="stylesheet" />
</head>
<body>
    <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
        <div class="container-fluid">
            <div class="navbar-header clearfix" >
                <a class="navbar-brand" asp-controller="DotNetNote" asp-action="Index">SASIM⭐</a>
                <a href="https://github.com/jinsujj"><img src="~/assets/img/GitHub-Mark-32px.png" alt="github" width="22" height="22"></a>
            </div>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                    aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse d-sm-inline-flex flex-sm-row-reverse">
                <partial name="_LoginPartial" />
                <ul class="navbar-nav">
                    <li class="nav-item"><a class="nav-link text-dark" asp-controller="DotNetNote" asp-action="Index">Blog</a></li>
                    @if (User.Identity.IsAuthenticated)
                    {
                        @*<li class="nav-item"><a class="nav-link text-dark" asp-controller="Home" asp-action="Index">Home</a></li>*@
                        @*<li class="nav-item"><a class="nav-link text-dark" asp-controller="Home" asp-action="Student">Student</a></li>*@
                    }
                </ul>
            </div>
        </div>
    </nav>
    <div class="container-fluid">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>
    <footer class="border-top footer text-muted">
        <div class="container">
            <p>
                &copy; @DateTime.Now.Year - JS_Blog<br />
                &nbsp; &nbsp; Powered by .Net Core 5<br />
            </p>
        </div>
    </footer>

    <!-- Prism-->
    <script src="/js/prism_coy.js"></script>
    <!--Font AweSome -->
    <script src="https://kit.fontawesome.com/3ec141240c.js" crossorigin="anonymous"></script>
    <!--Prety fy-->
    <script src="https://cdn.jsdelivr.net/gh/google/code-prettify@master/loader/run_prettify.js"></script>
    <!-- Toast Grid-->
    <script src="https://uicdn.toast.com/grid/latest/tui-grid.js"></script>

    @RenderSection("Scripts", required: false)
</body>
</html>
